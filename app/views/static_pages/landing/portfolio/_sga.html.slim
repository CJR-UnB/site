      .close[data-dismiss='modal']
        span &times;
      .title.text-left SGA - CJR
    .container
      .row
        .col-lg-8.col-lg-offset-2
          .modal-body
            ul.rslides
              li = image_tag 'sga/sga.png'
              li = image_tag 'sga/sga2.png'
              li = image_tag 'sga/sga3.png'
